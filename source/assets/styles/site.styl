html
	font-size 62.5%
	-webkit-font-smoothing antialiased
	height 100%

body
	font 1.6rem/2.4rem $sfProDisplay
	color #1F1F1F
	background #fff

.clear
	clearfix()


a
	color #016AA2
	text-decoration none

.btn
	display inline-block
	box-sizing border-box
	background #E63946
	border-radius 12px
	font-weight 700
	font-size 1.6rem
	line-height 2.4rem
	padding 16px 31px
	color #fff
	text-align center
	&:disabled
		opacity 0.48
		cursor default
	&:hover
	&:active
		background #C81927
		color #fff
	&.with-border
		background #fff
		border 2px solid #5C5C5C
		color #1F1F1F
		padding 14px 29px
		&:hover
		&:active
			border-color #E63946
			color #1F1F1F

button.btn
	border none
	margin 0
	font 700 1.6rem/2.4rem $sfProDisplay
	white-space nowrap
	height 56px

input.text
	box-sizing border-box
	display block
	width 100%
	height 56px
	margin 0
	padding 16px
	border 1px solid #C2C2C2
	background #fff
	border-radius 12px
	font 1.6rem/2.4rem $sfProDisplay
	color #1F1F1F
	&:focus
		border-color #0190DB
	&.dark
		background rgba(255,255,255,.08)
		color #fff
		&:focus
			border-color #0190DB
			background #fff
			color #1f1f1f

.checkbox-custom-wrap
	position relative
	font-size 1.6rem
	line-height 2rem
	padding-left 36px
	.checkbox-view
		display block
		box-sizing border-box
		width 24px
		height 24px
		border 1px solid #C2C2C2
		border-radius 8px
		text-align center
		position absolute
		top 0
		left 0
	input.checkbox
		display block
		width 100%
		height 100%
		opacity 0
		position absolute
		top 0
		left 0
		&:checked + .checkbox-view
			background url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACAAAAAgCAQAAADZc7J/AAAAYklEQVR42mNgGAUjF/xP+J9Aifb6/yCQQJl2cg2Aa28Y1U597fiSBnHaQaCebMdDDcBiBNF+/78AmxEkBR2mESSHPKoRZEUcwgiy4x1uBPnJBsmIBnJz/AKKtFOhyBoFpAEAjqbhxPGCDooAAAAASUVORK5CYII=") 50% 50% no-repeat #0190DB
			background-size 16px 16px
			border-color #0190DB
		&:hover + .checkbox-view
			border-color #0190DB
		&:disabled + .checkbox-view
			border-color #C2C2C2
	.checkbox-copy
		a
			color #5DC6FE


.h-fixed
	padding 0 16px
	max-width 1152px
	margin 0 auto
	@media $tablet
		padding 0 24px
	@media $phone
		padding 0 16px


.h-header
	&.header-sticky
		background #fff
		position sticky
		top 0
		z-index 1

.h-topbar-wrap
	background rgba(0,0,0,.76)
	color #fff
	@media $tablet
		position relative
		z-index 2
	@media $phone
		padding 0
		background #E63946

.topbar-center
	margin 0 auto
	height 80px
	display flex
	align-items center
	flex-wrap nowrap
	@media $tablet
		height 88px
	.reg
		display flex
		align-items center
		width calc(100% - 72px)
		.topbar-title
			flex-shrink 0
			width 330px
			font-size 1.4rem
			line-height 2rem
			margin-right 22px
