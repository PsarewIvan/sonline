		padding 12px
		color #fff

.h-topnav-wrap
	padding 24px 0
	@media $phone
		padding 36px 0

.topnav-center
	display flex
	align-items center
	@media $phone
		justify-content space-between
	.logo
		width 126px
		height 24px
		background url("data:image/png;base64,") 0 0 no-repeat
		background-size 126px 24px
		overflow hidden
		cursor pointer
		text-indent -9999px
		@media $tablet
			position relative
			z-index 2
.topnav
	display flex
	padding-left 24px
	@media $phone
		width 24px
.topnav-list
	display flex
	padding-left 24px
	li
		margin 0 0 0 32px
		font-weight 700
		@media $tablet
			margin 0
			overflow hidden
		a
			color #1f1f1f
			&:hover
				color #0190DB
.topnav-login
	margin-left auto
	@media $tablet
		position relative
		z-index 2
.topnav-phone
	font-weight 700
	margin-left 24px
	@media $tablet
		position relative
		z-index 2

.topnav-button
	display block
	background-color #1f1f1f
	position absolute
	top 11px
	height 2px
	width 24px
	transition transform 400ms cubic-bezier(0.23, 1, 0.32, 1)
	border-radius: 2px
	&:before
	&:after
		display block
		background-color #1f1f1f
		position absolute
		height 2px
		width 24px
		transition transform 400ms cubic-bezier(0.23, 1, 0.32, 1)
		border-radius: 2px
	&:before
		content ""
		margin-top -8px
	&:after
		content ""
		margin-top: 8px
#topnav-toggle
	display none
	& ~ .topnav-list
		@media $tablet
			box-sizing border-box
			position fixed
			top 0
			left 0
			z-index 1
			padding 0
			width 100%
			display block
			background #fff
		li
			@media $tablet
				height 0
				margin 0
				padding 0 24px
				border 0
				opacity 0
				transition opacity 400ms cubic-bezier(0.23, 1, 0.32, 1)
				font-size 2.8rem
				line-height 4rem
			@media $phone
				padding-left 16px
				padding-right 16px
	&:checked
		& + .topnav-button-container
				.topnav-button
					background rgba(255, 255, 255, 0)
					&:before
						margin-top 0px
						transform rotate(405deg)
					&:after
						margin-top 0px
						transform rotate(-405deg)
		& ~ .topnav-list
			@media $tablet
				height 100%
				padding 216px 0 24px
			@media $phone
				padding-top 168px
			li
				@media $tablet
					opacity 1
					box-sizing border-box
					height 88px
					padding-top 16px
					transition opacity 400ms cubic-bezier(0.23, 1, 0.32, 1)
				&.topnav-login
					@media $phone
						height 104px
						padding-top 32px
.topnav-button-container
	cursor pointer
	display flex
	width 24px
	height 24px
	@media $tablet
		position relative
		z-index 2

