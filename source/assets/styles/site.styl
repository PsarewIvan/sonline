			flex-shrink 0
			width 180px
			height 48px
			padding 8px 0
		.captcha
			flex-shrink 0
			margin-left auto
			img
				display block
	.topbar-closer
		flex-shrink 0
		width 48px
		height 48px
		background url("data:image/png;base64,") 0 0 no-repeat
		background-size 48px 48px
		margin-left auto
		overflow hidden
		cursor pointer
		text-indent -9999px
	.topbar-result
		display flex
		align-items center
		background url("data:image/png;base64,") 0 50% no-repeat
