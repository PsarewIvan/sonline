@import "_variables"
@import "fonts"
@import "reset"


html
	font-size 62.5%
	-webkit-font-smoothing antialiased
	height 100%

body
	font 1.6rem/2.4rem $sfProDisplay
	color #1F1F1F
	background #fff
	height 100%

.clear
	clearfix()


a
	color #016AA2
	text-decoration none

.oferta-container
	max-width 1376px
	margin-left auto
	margin-right auto
	padding 64px 32px
	@media $tablet
		padding-top 16px
	@media $phone
		padding-left 16px
		padding-right 16px

	h2
		font-size 36px
		line-height 48px
		font-weight 700
		margin 0 0 16px

	h3
		margin 0 0 16px
		font-size 20px
		font-weight 700
		line-height 28px

	p
		margin 0 0 16px

	ul
		margin 0 0 16px
		display flex
		flex-direction column
		gap 4px

	a
		color: #016AA2

	.oferta-table-title
		margin-top 64px

	.oferta-table
		overflow-x: auto
		margin-top 32px
		margin-bottom 32px

		table
			min-width 960px

			tr
				border-top 1px solid #D6D6D6

			tr:first-child
				td
					padding 20px 8px
					font-weight 600

			tr:last-child
				border-bottom 1px solid #D6D6D6

			td
				padding 16px 8px

			td.rowgr
					padding 12px 8px
					background-color rgba(0, 0, 0, 0.04)

			td:first-child
				padding-left 0 !important

			td:last-child
				padding-right 0 !important

			td:nth-child(5)
				text-align center

			td:nth-child(6)
				text-align center

.oferta
	max-width 720px
	margin-left auto
	margin-right auto

.btn
	display inline-block
	box-sizing border-box
	background #E63946
	border-radius 12px
	font-weight 700
	font-size 1.6rem
	line-height 2.4rem
	padding 16px 55px
	color #fff
	text-align center
	&:disabled
		opacity 0.48
		cursor default
	&:hover
	&:active
		background #C81927
		color #fff
	&.with-border
		background #fff
		border 2px solid #5C5C5C
		color #1F1F1F
		padding 14px 53px
		&:hover
		&:active
			border-color #E63946
			color #1F1F1F

button.btn
	border none
	margin 0
	font 700 1.6rem/2.4rem $sfProDisplay
	white-space nowrap
	height 56px

.b-btn-centered
	text-align center
	padding 48px 0 0
	@media $phone
		padding 16px 0 0

.form-fieldset__wrap
	margin 0 0 16px

.form-fieldset-btn__wrap
	margin 32px 0

.form-fieldset__wrap-flex-row
	display flex
	align-items flex-start
	gap 0 16px
	.flex-col
		width calc(50% - 8px)

.form-title__wrap
	padding 0 0 32px
	h2
		font-size 2.8rem
		line-height 4rem
		font-weight 700
		margin 0 0 4px

.form-success__wrap
	padding 0 0 32px
	h3
		font-size 2rem
		line-height 2.8rem
		font-weight 700
		margin 0 0 4px
		color #2B9927

.input-wrap
	position relative
	input.reset
		width 16px
		height 16px
		margin 0
		padding 0
		border none
		background none
		-webkit-appearance none
		position absolute
		top 50%
		right 16px
		transform translateY(-50%)
	input.text
		&:focus + input.reset
			background url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACAAAAAgCAQAAADZc7J/AAAAkElEQVR42u2USwrAIAxEPWpuoheQObKlSC225tsunXXeI+iQlHb+TaVKH2YqoaEhSzjyOcMo0AWCouNo4HZAkRQDL9KKrMKE8wozvla48LfCjc+KED4pYvhDUULFHssr7dTxiOJ+OrmdKq4X3NR5l2L972YFXxuTQm6dqqik1eZSyBepGNrJXiSCelQtMzvOHJuWALADA3RDAAAAAElFTkSuQmCC") 0 0 no-repeat
			background-size 16px 16px

.input-password-wrap
	.toggle-password
		display block
		width 24px
		height 24px
		background url("data:image/png;base64,") 0 0 no-repeat
		background-size 24px 24px
		position absolute
		top 50%
		right 16px
		transform translateY(-50%)
		cursor pointer
		&.eye
			background-image url("data:image/png;base64,")

input.text
	box-sizing border-box
	display block
	width 100%
	height 56px
	margin 0
	padding 16px
	border 1px solid #C2C2C2
	background #fff
	border-radius 12px
	font 1.6rem/2.4rem $sfProDisplay
	color #1F1F1F
	&:focus
		border-color #0190DB
	&.dark
		background rgba(255,255,255,.08)
		color #fff
		&:focus
			border-color #0190DB
			background #fff
			color #1f1f1f

.errors
	input.text
		border-color #FF3B38
	&-message
		font-size 1.4rem
		line-height 2rem
		margin 4px 0 0
		color #FF3B38

.checkbox-custom-wrap
	position relative
	font-size 1.6rem
	line-height 2rem
	padding-left 36px
	margin 12px 0
	.checkbox-view
		display block
		box-sizing border-box
		width 24px
		height 24px
		border 1px solid #C2C2C2
		border-radius 8px
		text-align center
		position absolute
		top 0
		left 0
	input.checkbox
		display block
		opacity 0
		position absolute
		top 0
		left 0
		&:checked + .checkbox-view
			background url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACAAAAAgCAQAAADZc7J/AAAAYklEQVR42mNgGAUjF/xP+J9Aifb6/yCQQJl2cg2Aa28Y1U597fiSBnHaQaCebMdDDcBiBNF+/78AmxEkBR2mESSHPKoRZEUcwgiy4x1uBPnJBsmIBnJz/AKKtFOhyBoFpAEAjqbhxPGCDooAAAAASUVORK5CYII=") 50% 50% no-repeat #0190DB
			background-size 16px 16px
			border-color #0190DB
		&:hover + .checkbox-view
			border-color #0190DB
		&:disabled + .checkbox-view
			border-color #C2C2C2
	.checkbox-copy
		a
			color #0190DB
	&.errors
		.checkbox-view
			border-color #FF3B38
.h-topbar-wrap
	.checkbox-custom-wrap
		.checkbox-copy
			a
				color #5DC6FE


.h-fixed
	padding 0 32px
	max-width 1376px
	margin 0 auto
	@media $phone
		padding 0 16px


.h-header
	&.header-sticky
		background #fff
		position sticky
		top 0
		z-index 100

.h-topbar-wrap
	background rgba(0,0,0,.76)
	color #fff
	@media $phone
		padding 0
		background #E63946

.topbar-center
	margin 0 auto
	height 80px
	display flex
	align-items center
	flex-wrap nowrap
	@media $tablet
		height 88px
	.reg
		display flex
		align-items center
		justify-content space-between
		width calc(100% - 72px)
		.topbar-title
			flex-shrink 0
			width 330px
			font-size 1.4rem
			line-height 2rem
			margin-right 22px
			@media $tablet
				width 206px
			strong
				display block
				font-size 2rem
				line-height 2.8rem
				font-weight 700
		.input-wrap
			margin 0 8px 0 0
			width 100%
		input.text
			height 48px
			padding-top 12px
			padding-bottom 12px
		button.btn
			flex-shrink 0
			width 180px
			height 48px
			padding 8px 0
			&.send
				width 150px
		.captcha
			flex-shrink 0
			img
				display block
	.topbar-closer
		flex-shrink 0
		width 48px
		height 48px
		background url("data:image/png;base64,") 0 0 no-repeat
		background-size 48px 48px
		margin-left auto
		overflow hidden
		cursor pointer
		text-indent -9999px
	.topbar-result
		display flex
		align-items center
		background url("data:image/png;base64,") 0 50% no-repeat
		background-size 48px 48px
		padding 0 0 0 58px
		font-size 2rem
		line-height 2.8rem
		font-weight 700
		color #99CE97
		min-height 48px
.topbar-free-trial
	@media $phone
		display block
		background #E63946
		font-size 1.6rem
		line-height 2.4rem
		font-weight 700
		text-align center
		padding 12px
		color #fff

.h-topnav-wrap
	padding 24px 0
	@media $phone
		padding 36px 0

.topnav-center
	display flex
	align-items center
	@media $phone
		justify-content space-between
	.logo
		width 192px
		height 48px
		background url(../i/logo.svg) 0 0 no-repeat
		background-size 192px 48px
		overflow hidden
		cursor pointer
		text-indent -9999px
		flex-shrink 0
		@media $tablet
			position relative
			z-index 2
.topnav
	display flex
	padding-left 24px
	@media $onlysmalldesktop
		padding-left 0
	@media $phone
		width 24px
.topnav-list
	display flex
	padding-left 24px
	@media $onlysmalldesktop
		padding-left 10px
	li
		margin 0 0 0 32px
		font-weight 700
		@media $onlysmalldesktop
			margin-left 15px
		@media $tablet
			margin 0
		a
			color #1f1f1f
			&:hover
				color #0190DB
		.topnav-logged__username
			a
				color #0190DB
		&.active
			a
				color #0190DB
.topnav-login
	margin-left auto
	@media $tablet
		position relative
		z-index 2
	.btn
		@media $tablet
			padding-left 29px
			padding-right 29px
		& + .btn
			margin-left 16px
	@media $phone
		display none
.topnav-phone
	font-weight 700
	margin-left 24px
	@media $tablet
		position relative
		z-index 2

.topnav-button
	display block
	background-color #1f1f1f
	position absolute
	top 11px
	height 2px
	width 24px
	transition transform 400ms cubic-bezier(0.23, 1, 0.32, 1)
	border-radius: 2px
	&:before
	&:after
		display block
		background-color #1f1f1f
		position absolute
		height 2px
		width 24px
		transition transform 400ms cubic-bezier(0.23, 1, 0.32, 1)
		border-radius: 2px
	&:before
		content ""
		margin-top -8px
	&:after
		content ""
		margin-top: 8px
#topnav-toggle
	display none
	& ~ .topnav-list
		@media $tablet
			box-sizing border-box
			position fixed
			top 192px
			left 0
			z-index 1
			padding 0
			width 100%
			display none
			flex-direction column
			justify-content space-around
			background #fff
			overflow auto
		@media $phone
			top 96px
			justify-content flex-end
		li
			@media $tablet
				margin 0
				padding 0 24px
				border 0
				opacity 0
				transition opacity 400ms cubic-bezier(0.23, 1, 0.32, 1)
				font-size 2.8rem
				line-height 4rem
			@media $phone
				padding-left 16px
				padding-right 16px
	&:checked
		& + .topnav-button-container
				.topnav-button
					background rgba(255, 255, 255, 0)
					&:before
						margin-top 0px
						transform rotate(405deg)
					&:after
						margin-top 0px
						transform rotate(-405deg)
		& ~ .topnav-list
			@media $tablet
				display flex
				height calc(100% - 192px)
				padding 0
			@media $phone
				height calc(100% - 96px)
			li
				@media $tablet
					opacity 1
					box-sizing border-box
					padding-top 4px
					padding-bottom 4px
					transition opacity 400ms cubic-bezier(0.23, 1, 0.32, 1)
				&.topnav-login
					@media $phone
						padding-top 48px
						padding-bottom 32px
						order 2
						.btn
							display block
							& + .btn
								margin 16px 0 0
				&.topnav-phone
					@media $phone
						font-size 2rem
						line-height 2.8rem
						padding 0
						text-align center
.topnav-button-container
	cursor pointer
	display flex
	width 24px
	height 24px
	@media $tablet
		position relative
		z-index 2
.topnav-logged
	margin-left auto
	display flex
	align-items center
	height 56px
	&__username
		padding 0 16px 0 8px
		font-weight 700
		color #0190DB
		a
			color #0190DB
	&__sign-out
		display flex
		align-items center
		color rgba(92,92,92,1)
		@media $phone
			margin-left auto
	.ph-sign-out
		font-size 2.4rem
		color rgba(92,92,92,1)
		@media $phone
			font-size 3.2rem


.h-content-wrap
	overflow hidden


.b-offer-wrap
	box-sizing border-box
	min-height calc(100vh - 184px)
	position relative
	display flex
	align-items center
	justify-content center
	@media $tablet
		padding 48px 0 520px
		min-height auto
	@media $phone
		padding 32px 0
	&:before
		display block
		content ""
		width 1072px
		height 756px
		max-height 140%
		position absolute
		top 50%
		left 50%
		z-index -1
		transform translate(-150px, -50%)
		background url(../i/offer-mockup.png) 0 0 no-repeat
		background-size auto 100%
		@media $tablet
			width 948px
			height 668px
			max-height none
			background-size cover
			top auto
			bottom -60px
			transform translateX(-50%)
			background-size 948px 668px
		@media $phone
			left 0
			bottom auto
			top 0
			transform translateX(-97px)
		@media $smallphone
			top 32px
	.h-fixed
		box-sizing border-box
		width 100%
		padding-top 20px
		padding-bottom 20px
		@media $tablet
			padding-top 0
			padding-bottom 0
	&.region-var
		@media $phone
			&:before
				top 32px
			.offer-copy h1
				max-width 380px
				margin-bottom 542px
		@media $smallphone
			&:before
				top 64px
			.offer-copy h1
				max-width 270px
				margin-bottom 574px
.offer-copy
	width 480px
	position relative
	@media $tablet
		width auto
	h1
		font-size 3.6rem
		line-height 4.8rem
		font-weight 700
		margin 0 0 40px
		letter-spacing -0.015em
		@media $phone
			font-size 2.4rem
			line-height 3.2rem
			margin-bottom 510px
		@media $smallphone
			max-width 290px
.offer-list
	padding 0 0 36px
	li
		position relative
		margin 0 0 4px
		padding 0 0 0 28px
		&:after
			display block
			content ""
			width 16px
			height 24px
			background url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACAAAAAwCAYAAABwrHhvAAAAfElEQVR4Ae2WAQaAMBhGu0w1W4faTeoWvwTdtAqSX2XgK3qPAXzvMWYVAAAA/IpoIUer8yvybmr6NLbLfmQRXi4PuJaHATnyb8vdY6GXHwPboEzuA84R8jtPFmYfIZAXRAjkBREi+VOETH4XIZX7CL3cR+jl+i8VAACAnBVRQNesa3S9DQAAAABJRU5ErkJggg==") 0 0 no-repeat
			background-size 16px 24px
			position absolute
			top 0
			left 0
.offer-form
	display flex
	flex-wrap wrap
	@media $phone
		display block
	.offer-form__arrow
		width 100%
		font-size 1.4rem
		line-height 2rem
		margin 0 0 16px
		position relative
		color #5c5c5c
		@media $phone
			display block
		strong
			display block
			font-size 2rem
			line-height 2.8rem
			font-weight 700
			color #1f1f1f
		&:after
			display block
			content ""
			width 42px
			height 88.5px
			background url("data:image/png;base64,") 0 0 no-repeat
			background-size 42px 88.5px
			position absolute
			top 12px
			left -52px
			@media $tablet
				display none
	.input-wrap
		width 293px
		@media $tablet
			width auto
			flex-grow 1
			margin-right 8px
		@media $phone
			width 100%
			margin 0 0 8px
	.btn
		margin-left auto
		@media $phone
			width 100%
	.offer-form__checkbox-holder
		width 100%
		height 56px
		display flex
		align-items center
	.offer-form__btn-wrap
		width 100%
	.offer-result
		display flex
		align-items center
		background url("data:image/png;base64,") 0 0 no-repeat
		background-size 64px 64px
		padding 80px 0 0
		font-size 2rem
		line-height 2.8rem
		font-weight 700
		color #2B9927
	.captcha
		margin 0 0 16px

.b-advantages-wrap
	padding 64px 0
	@media $tablet
		padding 36px 0
	@media $phone
		padding 20px 0
.advantages-list
	display flex
	justify-content space-between
	@media $tablet
		flex-wrap wrap
	li
		width calc(25% - 9px)
		border-radius 12px
		background #fff
		box-shadow 0px 8px 32px 0px rgba(0, 0, 0, 0.04), 0px 16px 64px 0px rgba(0, 0, 0, 0.03), 0px 32px 96px 0px rgba(0, 0, 0, 0.02), 0px 96px 120px 0px rgba(0, 0, 0, 0.01), 0px 240px 120px 0px rgba(0, 0, 0, 0.00)
		@media $tablet
			margin 0 0 12px
			width calc(50% - 6px)
	&__inner
		padding 32px
		@media $phone
			padding 16px
	&__title
		font-size 4.8rem
		line-height 6.4rem
		font-weight 700
		display block
		@media $phone
			font-size 2.8rem
			line-height 4rem
	&__copy
		font-size 2rem
		line-height 2.8rem
		display block
		@media $phone
			font-size 1.4rem
			line-height 2rem

.b-section-wrap
	padding 0 0 64px
	@media $phone
		padding 0 0 32px
.section-title
	padding 0 0 48px
	@media $phone
		padding 0 0 16px
.section-h2
	font-size 2.8rem
	line-height 4rem
	font-weight 700
	text-align center
	margin 0
	.colored
		color #E63946
.section-h3
	font-size 2.8rem
	line-height 4rem
	font-weight 700
	margin 0
	text-align center
	@media $phone
		font-size 2.4rem
		line-height 3.2rem
.section-copy
	font-size 2rem
	line-height 2.8rem
	font-weight 700
	color #016AA2
	@media $phone
		font-size 2rem
		line-height 2.8rem
		margin 8px 0 0
.section-footnotes
	padding 24px 0 0
	max-width 66%
	@media $tablet
		max-width 100%
	@media $phone
		padding-top 20px
	li
		font-size 1.2rem
		line-height 1.6rem
		color #5C5C5C
		margin 12px 0 0
.section-tabs
	&-wrap
		text-align center
		padding 32px 0 0
		@media $phone
			padding 16px 0 0
	&-list
		display inline-flex
		align-items center
		background rgba(0, 0, 0, 0.08)
		border-radius 16px
		padding 4px
	&-list-item
		display block
		padding 12px 24px
		font-size 1.6rem
		line-height 2.4rem
		font-weight 700
.section-tabs-list
	.active
		.section-tabs-list-item
			background #fff
			border-radius 12px
			box-shadow 0px 4px 4px 0px rgba(0, 0, 0, 0.04)


.functionality-cols
	&-wrap-overfow
		overflow-y hidden
		@media $phone
			margin-bottom -116px
.functionality-col
	&__list
		columns 5
		column-gap 12px
		transform translateY(-208px)
		@media $tablet
			transform translateY(-148px)
		@media $phone
			columns 3
			transform translateY(-116px)
		li
			box-sizing border-box
			padding 26px 24px 0
			height 220px
			border-radius 12px
			background #fff
			box-shadow 0px 8px 32px 0px rgba(0, 0, 0, 0.04), 0px 16px 64px 0px rgba(0, 0, 0, 0.03), 0px 32px 96px 0px rgba(0, 0, 0, 0.02), 0px 96px 120px 0px rgba(0, 0, 0, 0.01), 0px 240px 120px 0px rgba(0, 0, 0, 0.00)
			margin 0 0 12px
			text-align center
			font-weight 600
			color #5C5C5C
			break-inside avoid-column
			@media $tablet
				height 160px
				font-size 1.4rem
				line-height 2rem
				padding 20px 8px 0
			@media $phone
				height 128px
				padding 8px 8px 0
				font-size 1.2rem
				line-height 1.6rem
			&.empty
				.functionality-col__list-img
					display none
					@media $phone
						display block
			&.empty-for-phone
				.functionality-col__list-img
					@media $phone
						display none
			&.break-phone
				@media $phone
					-webkit-column-break-before always
					column-break-before always
					break-before column
			&:nth-child(4)
			&:nth-child(10)
				padding-top 124px
				height 318px
				@media $tablet
					padding-top 100px
					height 240px
				@media $phone
					padding-top 8px
					height 128px
			&:nth-child(7)
				@media $phone
					padding-top 84px
					height 204px
	&__list-img
		width 120px
		height 120px
		margin 0 auto
		background-position 0 0
		background-repeat no-repeat
		background-size 120px 120px
		@media $tablet
			width 80px
			height 80px
			background-size 80px 80px
		&.calendar
			background-image url(../i/calendar-icon.png)
		&.clients
			background-image url(../i/clients-icon.png)
		&.broadcast
			background-image url(../i/broadcast-icon.png)
		&.stock
			background-image url(../i/trolley-icon.png)
		&.loyalty
