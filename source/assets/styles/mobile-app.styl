@import "_variables"
.mobile-app-frame
	padding-top 20px
	.btn
		display inline-block
		background #1A73E8
		border 1px solid #1A73E8
		border-radius 999px
		font-weight 600
		padding 12px 24px
		color rgba(255,255,255,.9)
		@media $phone
			font-size 1.4rem
			line-height 1.8rem
			padding 10px 12px
		@media $smallphone
			font-size 1.2rem
			padding 10px
		&:hover
			color #1A73E8
			background none
		&:disabled
			background none
			border-color rgba(0,0,0,.3)
			color rgba(0,0,0,.6)
	button.btn
		font 2.4rem $sfUiDisplay
		padding 16px 32px
		border-radius 32px

	.h-wrapper
		box-sizing border-box
		overflow hidden
		min-height 100%
		position relative
		padding 0 0 100px
		@media $tablet
			padding 0

	.h-fixed
		box-sizing border-box
		max-width 1400px
		margin 0 auto
		padding 0 64px
		position relative
		@media $tablet
			padding 0 32px
		@media $phone
			padding 0 16px

	.h-header
		clearfix()
		padding 29px 0 37px
		position relative
		@media $tablet
			padding 20px 0 120px
		@media $phone
			padding 12px 0 28px
		&:before
			box-sizing border-box
			display block
			content ""
			width 480px
			height 480px
			position absolute
			top -280px
			left -120px
			border 1px dashed rgba(0,0,0,.1)
			border-radius 50%
			@media $tablet
				top -300px
			@media $phone
				display none

	.logo
		float left
		margin 3px 0 0
		width 198px
		height 40px
		text-indent -9999px
		outline none
		overflow hidden
		background url("data:image/svg+xml;base64,") 0 0 no-repeat
		background-size cover
		@media $phone
			width 138px
			height 28px
			margin 5px 0 0
		@media $smallphone
			width 120px
			height 24px
			margin-top 8px

	.site-nav
		float right
		li
			float left
			font-weight 600
			margin 12px 0 0 32px
			@media $tablet
				display none
			&.with-btn
				margin-top 0
				@media $tablet
					display block
					margin-left 0


	.promo
		&-wrap
			max-width 1400px
			min-height 640px
			margin 0 auto
			display flex
			flex-wrap wrap
			align-items center
			position relative
			@media $tablet
				display block
				margin 0 0 3px
			@media $phone
				min-height auto
		&-copy
			box-sizing border-box
			padding 0 0 70px 64px
			width 57%
			max-width 800px
			font-size 1.6rem
			line-height 2.2rem
			font-weight 500
			@media $tablet
				padding 0 0 455px 32px
				width 100%
				max-width 80%
			@media $phone
				width 100%
				max-width 100%
				padding 0 16px 370px 16px
				font-size 1.3rem
				line-height 1.6rem
			h1
				font-size 5.6rem
				line-height 6.4rem
				font-weight 500
				margin 0 0 12px
				max-width 620px
				@media $phone
					max-width 100%
					font-size 3.2rem
					line-height 3.6rem
					margin 0 0 8px
			h2
				font-size 2rem
				color rgba(0,0,0,.6)
				margin 0 0 53px
				@media $phone
					font-size 1.6rem
					margin 0 0 16px
		&-banner
			border-radius 12px
			background rgba(255, 192, 0, 0.50)
			padding 24px
			font-size 20px
			line-height 28px
			font-weight 700
			color #E63946
			margin 32px 0
			@media $phone
				margin 16px 0
			p
				margin 0 0 24px
			&-inner
				max-width 70%
				width 472px
				@media $tablet
					max-width 100%
				@media $phone
					width 100%
					max-width 100%
			&-btn
				display inline-block
				border-radius 12px
				border 2px solid #E63946
				background #fff
				font-size 16px
				line-height 24px
				font-weight 700
				color #C81927
				padding 16px 32px
				&:hover
					border-color #5c5c5c
		&-list
			li
				margin 0 0 12px
				padding 5px 0 5px 40px
				position relative
				@media $phone
					margin 0 0 8px
					padding 4px 0 4px 32px
				&:after
					display block
					content ""
					width 32px
					height 32px
					background url("data:image/svg+xml;base64,") 0 0 no-repeat
					background-size cover
					position absolute
					top 50%
					left 0
					transform translateY(-50%)
					@media $phone
						width 24px
						height 24px
				&:last-child
					margin 0
		&-picture
			width 640px
			height 640px
			border-radius 50%
			background-position 50% 50%
			background-repeat no-repeat
			background-size cover
			position absolute
			top 50%
			left 59%
			transform translateY(-50%)
			@media $tablet
				width 560px
				height 560px
				top auto
				bottom 0
				left auto
				right -120px
				transform none
			@media $phone
				width 400px
				height 400px
				right -140px
			&-screen
				width 480px
				height 480px
				background rgba(26,115,232,.8)
				border-radius 50%
				position absolute
				top 64px
				left -200px
				@media $tablet
					width 400px
					height 400px
					top 128px
					left -170px
				@media $phone
					width 320px
					height 320px
					top 40px
					left -80px
				&-title
					width 160px
					height 160px
					border-radius 50%
					background rgba(230,8,106,.8)
					box-shadow 0 4px 16px rgba(192,7,89,.5)
					position absolute
					bottom 48px
					left -48px
					font-size 2rem
					font-weight bold
					line-height 2.4rem
					display flex
					align-items center
					text-align center
					text-transform uppercase
					color #fff
					@media $tablet
						top 48px
						bottom auto
						left -96px
					@media $phone
						width 104px
						height 104px
						font-size 1.6rem
						line-height 2rem
						top 9px
						left 0
				&-mask
					width 100%
					height 100%
					border-radius 50%
					position absolute
					top 0
					left 0
					overflow hidden
					img
						display block
						width 248px
						height 495px
						position absolute
						top 64px
						left 50%
						transform translateX(-50%)
						box-shadow 0 4px 16px rgba(0,0,0,.5)
						border-radius 38px
						@media $tablet
							width 208px
							height 416px
							border-radius 32px
							top 58px
						@media $phone
							width 168px
							height 336px
							top 39px

	.h-content
		padding 0 0 32px
		h1
		h2
			font-size 3.2rem
			font-weight bold
			text-align center
			margin 0 0 24px
			@media $phone
				font-size 2.4rem
				margin 0 0 16px

	.content
		&-cols-wrap
			display flex
			flex-wrap wrap
			justify-content space-between
			padding 0 0 40px
			position relative
			@media $phone
				display block
				padding 0 0 32px
			&.var2
				align-items center
				padding 0
				@media $phone
					padding-top 1px
				.content-col
					margin 64px 0 0
					padding 0
					@media $tablet
						width 52%
						margin 52px 0 0
						&:first-child
							width 400px
							position relative
							left -96px
							margin-right -96px
					@media $phone
						width 100%
						margin 0 auto 8px
						&:first-child
							width 100%
							height 400px
							left auto
							margin 52px auto 8px
				&:nth-child(even)
					.content-col:first-child
						order 2
						@media $tablet
							left auto
							right -96px
							margin 0 0 0 -96px
						@media $phone
							right auto
							margin 52px auto 8px
			&.cols-in-small
				@media $tablet
					padding 0 0 53px
					flex-direction column
					.content-col
						width 100%
						padding 0 0 8px
				@media $phone
					padding 0 0 24px
			h3
				font-size 2.4rem
				font-weight bold
				margin 0 0 16px
				@media $phone
					font-size 1.8rem
					line-height 2.2rem
		&-cols-wrap-padding
			padding 0 0 64px
			@media $phone
				padding 0 0 24px
		&-cols-wrap-padding-2
			padding 108px 0 0
			@media $phone
				padding-top 80px
		&-col
			width 47%
			position relative
			padding 0 0 24px
			@media $phone
				width 100%
				padding 0 0 104px
				&:last-child
					padding 0
		&-col-screen
			width 480px
			height 480px
			border-radius 50%
			position relative
			margin 0 auto
			@media $tablet
				width 400px
				height 400px
			@media $phone
				position absolute
				top 0
				left 50%
				transform translateX(-50%)
			img
				display block
				width 248px
				height 495px
				position absolute
				top -63px
				left 50%
				transform translateX(-50%)
				box-shadow 0 4px 16px rgba(0,0,0,.5)
				border-radius 38px
				@media $tablet
					width 208px
					height 416px
					border-radius 32px
					top -52px
		&-col-feedback
			clearfix()
			@media $tablet
				margin 0 0 16px
			&-logo
				float left
				width 64px
				height 64px
				border-radius 50%
				margin 0 16px 0 0
				@media $phone
					width 48px
					height 48px
					margin 0 8px 0 0
			&-copy
				overflow hidden
			&-name
				font-size 1.6rem
				font-weight 500
				margin 12px 0 0
				@media $phone
					font-size 1.3rem
					line-height 1.6rem
		&-col-price
			box-sizing border-box
			height 100%
			border 1px dashed rgba(0,0,0,.1)
			border-radius 32px
			padding 116px 32px 32px
			position relative
			@media $tablet
				padding 116px 16px 16px
			@media $phone
				padding-top 92px
			&-summ
				width 200px
				height 200px
				border-radius 50%
				position absolute
				top -100px
				left 50%
				transform translateX(-50%)
				color rgba(255,255,255,.9)
				font-size 3.2rem
				line-height 3.8rem
				font-weight bold
				text-align center
				display flex
				flex-wrap wrap
				align-items center
				text-align center
				@media $phone
					width 160px
					height 160px
					top -80px
					font-size 2.4rem
					line-height 3rem
				.big-fs
					display block
					width 100%
				.small-fs
					display block
					width 100%
					font-size 2.4rem
					line-height 120%
					@media $phone
						font-size 1.8rem
			&-center
				padding 16px 0 0
				text-align center

	.with-circles
		&:before
		&:after
			display block
			content ""
			width 320px
			height 320px
			position absolute
			top -174px
			left -144px
			background rgba(0,0,0,.03)
			border-radius 50%
			@media $tablet
				top -73px
				left -112px
			@media $phone
				display none
		&:after
			width 64px
			height 64px
			top -190px
			left 128px
			@media $tablet
				top -89px
				left 160px
		&.reverse
			&:before
			&:after
				left auto
				right -144px
				top auto
				bottom -203px
				@media $tablet
					display none
			&:after
				right 128px
				bottom 70px
		&.h-oferta
			&:before
				top auto
				bottom 410px
			&:after
				top auto
				bottom 681px

	.pink
		background rgba(230,8,106,.8)
	.orange
		background rgba(250,85,2,.8)
	.green
		background rgba(28,132,0,.8)
	.blue
		background rgba(26,115,232,.8)

	.content-order-app-wrap
		max-width 544px
		margin 0 auto
		fieldset
			margin 0 0 12px
		label
			display inline-block
			font-size 14px
			line-height 20px
			color rgba(0,0,0,.6)
			@media $phone
				font-size 1.1rem
				line-height 1.4rem
		input.text
			box-sizing border-box
			display block
			margin 0
			padding 0
			width 100%
			height 55px
			background none
			border none
			border-bottom 1px solid rgba(0,0,0,.1)
			font 2.4rem $sfUiDisplay
			color rgba(0,0,0,.9)
			@media $phone
				height 38px
				font-size 1.8rem
			&:focus
				border-bottom-color #1A73E8
	.content-order-app-center
		padding 12px 0 0
		text-align center
		@media $phone
			padding-top 4px
	.radio-list
		padding 12px 0 0
		li
			display inline-block
			vertical-align top
			margin 0 45px 0 0
			position relative
			@media $phone
				margin 0 21px 0 0
		input.radio
			position absolute
			top 0
			left 0
			opacity 0
		label
			display block
			position relative
			padding 0 0 0 44px
			font-size 24px
			line-height 32px
			color rgba(0,0,0,.6)
			@media $phone
				font-size 18px
				line-height 24px
				padding 0 0 0 32px
			&:after
				display block
				content ""
				width 16px
				height 16px
				border 7px solid #fff
				box-shadow 0 0 0 1px rgba(0,0,0,.3)
				border-radius 50%
				position absolute
				top 0
				left 0
				background transparent
				@media $phone
					width 12px
					height 12px
					border-width 6px
		input.radio:checked + label
			color rgba(0,0,0,.9)
			&:after
				background #1A73E8
				box-shadow 0 0 0 1px #1A73E8
	.thankyou
		padding 35px 0
		text-align center
		@media $phone
			padding 0
		h2
			font-size 24px
			line-height 30px
			font-weight normal
			color #1C8400
			margin 0 0 8px
			padding 80px 0 0
			background url("data:image/svg+xml;base64,") 50% 0 no-repeat
			background-size 64px 64px
			@media $phone
				font-size 18px
				line-height 22px
				background-size 48px 48px
				padding 60px 0 0
				margin 0 0 4px

	.h-oferta
		padding 10px 0 0
		h2
		h3
			font-size 20px
			line-height 24px
			margin 0 auto 24px
			text-align center
			max-width 600px
			@media $phone
				max-width 100%
				font-size 16px
				line-height 20px
				margin-left 12px
	.oferta-list
		li
			position relative
			margin 0 0 8px
			padding 0 0 0 40px
			@media $phone
				padding-left 36px
		&-numb
			display block
			position absolute
			top 0
			left 0
		ul
			padding 8px 0
			li
				padding 0 0 0 20px
				@media $phone
					padding-left 16px
				&:after
					display block
					content "‚óè"
					position absolute
					top 0
					left 0
	.requisites
		li
			padding 4px 8px
			@media $phone
				padding 4px
			&:nth-child(even)
				background rgba(0,0,0,.03)
		&-title
			display block
			font-size 14px
			line-height 20px
			color rgba(0,0,0,.6)
			@media $phone
				font-size 11px
				line-height 14px
		&-copy
			display block

	.app-example
		background #fff
		text-align center
		color #333
	.app-example-inner
		padding 80px 0 165px
	.apps-icons
		margin 0
		padding 0
		list-style none
		&.android
			li
				img
					border-radius 10px
		li
			display inline-block
			vertical-align top
			width 19.6%
			margin 0 0 45px
			padding 0
			list-style none
			@media $tablet
				width 32.6%
			@media $phone
				width 49.6%
			img
				display block
				margin 0 auto 25px
				box-shadow 5px 5px 35px rgba(0,0,0,.2)
				border-radius 28px
			span
				display block
				margin 0 15px
				font-size 18px
				line-height 120%
		a
			color #333